<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE frame2-config PUBLIC
  "-//Megatome Technologies//DTD Frame2 Configuration 1.1//EN"
  "http://frame2.sourceforge.net/dtds/frame2-config_1_1.dtd">

<frame2-config>
   <global-forwards>
   </global-forwards>
   <events>
   </events>
   <event-mappings>
   </event-mappings>
   <event-handlers>
      <event-handler 
         name="event-handler1"
         type="com.frame2.eventHandler1">
         <forward name="forward1" type="HTMLResource" path="path1" />
      </event-handler>
      <event-handler 
         name="event-handler2"
         type="com.frame2.eventHandler2">
         <forward name="forward1" type="HTMLResource" path="path1" />
         <forward name="forward2" type="event" path="path2" />
      </event-handler>
      <event-handler 
         name="event-handler3"
         type="com.frame2.eventHandler3">
         <init-param name="param1"  value="val1"/>
         <init-param name="param2"  value="val2"/>
      </event-handler>  
   </event-handlers>
</frame2-config>